<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>18</title>
</head>
<body>
    <script>
        var num1 = parseInt(prompt("Digite o primeiro número inteiro:"));
        var num2 = parseInt(prompt("Digite o segundo número inteiro:"));
        var temp, resultado = "";

        if (num1 > num2) {
            temp = num1;
            num1 = num2;
            num2 = temp;
        }

        for (var i = num1 + 1; i < num2; i++) {
            resultado += i + " ";
        }

        alert("Os números inteiros no intervalo entre " + num1 + " e " + num2 + " são:\n" + resultado);
    </script>
